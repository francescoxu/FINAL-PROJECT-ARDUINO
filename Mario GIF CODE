#include <FastLED.h>

#define NUM_LEDS 64
#define DATA_PIN 3 
#define COLOR_ORDER GRB              
CRGB leds[NUM_LEDS];
int d = 180;

int r = 234;
int g = 192;
int b = 134;



void setup() {
  // put your setup code here, to run once:
  FastLED.addLeds<WS2811,DATA_PIN,COLOR_ORDER>(leds,NUM_LEDS);
  FastLED.setBrightness(1);
 
}

void loop() {
 
  // put your main code here, to run repeatedly:
    



    //SecondFrame
    leds[2] = CRGB::Violet;
    leds[3] = CRGB::Brown; 
    leds[4] = CRGB::Brown;
    leds[9] = CRGB::DarkBlue;
    leds[10] = CRGB::Blue;
    leds[11] = CRGB::Blue;
    leds[12] = CRGB::White;
    leds[13] = CRGB::Red;
    leds[17] = CRGB::DarkBlue; 
    leds[18] = CRGB::Blue;
    leds[19] = CRGB::Yellow;
    leds[20] = CRGB::Red;
    leds[21] = CRGB::Red;
    leds[25] = CRGB(r,g,b);
    leds[26] = CRGB(r,g,b);
    leds[27] = CRGB(r,g,b); 
    leds[28] = CRGB::Brown;
    leds[32] = CRGB(r,g,b);
    leds[33] = CRGB::Brown;
    leds[34] = CRGB::Brown;
    leds[35] = CRGB(r,g,b);
    leds[36] = CRGB(r,g,b);
    leds[37] = CRGB::Brown; 
    leds[41] = CRGB(r,g,b);
    leds[43] = CRGB::Brown;
    leds[44] = CRGB(r,g,b);
    leds[45] = CRGB::Brown;

    leds[58] = CRGB::Red;
    leds[59] = CRGB::Red;
    leds[60] = CRGB::Red;

    for(int dot = 48; dot <53; dot++){
      
    leds[dot] = CRGB::Red;
    }



    FastLED.show();
    fill_solid( leds, NUM_LEDS, CRGB::Black);
    delay(d);

    //FRAME 3
    leds[2] = CRGB::Brown;
    leds[5] = CRGB::Violet; 
    leds[8] = CRGB::Purple;
    leds[9] = CRGB::DarkBlue;
    leds[10] = CRGB::Blue;
    leds[11] = CRGB::Blue;
    leds[12] = CRGB::Blue;
    leds[13] = CRGB::Blue;
    leds[15] = CRGB::White;
    leds[17] = CRGB::Orange; 
    leds[18] = CRGB::Blue;
    leds[19] = CRGB::Blue;
    leds[20] = CRGB::Yellow;
    leds[21] = CRGB::Red;
    leds[22] = CRGB::Red;
    leds[25] = CRGB(255,205,148);
    leds[26] = CRGB(255,205,148);
    leds[27] = CRGB(255,205,148); 
    leds[28] = CRGB(255,205,148);
    leds[32] = CRGB::Brown;
    leds[33] = CRGB(255,205,148);
    leds[34] = CRGB::Brown;
    leds[35] = CRGB::Brown;
    leds[36] = CRGB(255,205,148);
    leds[37] = CRGB(255,205,148); 
    leds[42] = CRGB(255,205,148);
    
    leds[44] = CRGB::Brown;
    leds[45] = CRGB(255,205,148);

    leds[57] = CRGB::Red;
    leds[58] = CRGB::White;
    leds[59] = CRGB::Red;
    leds[60] = CRGB::Red;

     for(int dot = 48; dot <53; dot++){
      
    leds[dot] = CRGB::Red;
    }

    FastLED.show();
    fill_solid( leds, NUM_LEDS, CRGB::Black);
    delay(d);

    
    //FRAME 4
    leds[1] = CRGB::Brown;
    leds[9] = CRGB::DarkBlue;
    leds[10] = CRGB::Blue;
    leds[11] = CRGB::Blue;
    leds[12] = CRGB::Blue;
    leds[13] = CRGB::Blue;
    leds[14] = CRGB::Purple;
    leds[16] = CRGB::White;
    leds[17] = CRGB::Orange; 
    leds[18] = CRGB::Blue;
    leds[19] = CRGB::Blue;
    leds[20] = CRGB::Yellow;
    leds[21] = CRGB::Red;
    leds[23] = CRGB::White;
    leds[25] = CRGB(255,205,148);
    leds[26] = CRGB(255,205,148);
    leds[27] = CRGB(255,205,148); 
    leds[28] = CRGB(255,205,148);
    leds[29] = CRGB::Red;
    leds[30] = CRGB::Red;
    leds[32] = CRGB::Brown;
    leds[33] = CRGB(255,205,148);
    leds[34] = CRGB::Brown;
    leds[35] = CRGB::Brown;
    leds[36] = CRGB(255,205,148);
    leds[37] = CRGB(255,205,148); 
    leds[42] = CRGB(255,205,148);
    
    leds[44] = CRGB::Brown;
    leds[45] = CRGB(255,205,148);

    leds[57] = CRGB::Red;
    leds[58] = CRGB::White;
    leds[59] = CRGB::Red;
    leds[60] = CRGB::Red;

     for(int dot = 48; dot <53; dot++){
      
    leds[dot] = CRGB::Red;
    }

    FastLED.show();
    fill_solid( leds, NUM_LEDS, CRGB::Black);
    delay(d);

    //FRAME 5
     leds[1] = CRGB(32,156,101);

    leds[5] = CRGB::Brown;
    leds[28] = CRGB::Brown; 
    leds[33] = CRGB::Brown;
    leds[34] = CRGB::Brown;
    leds[37] = CRGB::Brown;; 
    leds[43] = CRGB::Brown;; 
    leds[45] = CRGB::Brown;; 
    leds[9] = CRGB(46,28,78); //DARK blue
    leds[10] = CRGB::Blue;
    leds[11] = CRGB::Blue;
    leds[12] = CRGB::Blue;
    leds[13] = CRGB::Blue;
    leds[18] = CRGB::Blue;
    leds[19] = CRGB::Blue;
    leds[15] = CRGB::White;
    leds[57] = CRGB(255,255,255);
    leds[16] = CRGB(117,129,157); //Light Purple
    leds[17] = CRGB::Orange; //Orange
    leds[20] = CRGB::Yellow; //yellow
    leds[24] = CRGB::Yellow; //yellow
    leds[21] = CRGB::Red;
    leds[22] = CRGB::Red;
    leds[58] = CRGB::Red; 
    leds[59] = CRGB::Red;
    leds[60] = CRGB::Red;
    //Skin COLOR
    leds[25] = CRGB(255,205,148);
    leds[26] = CRGB(255,205,148);
    leds[27] = CRGB(255,205,148);
    leds[35] = CRGB(255,205,148);
    leds[36] = CRGB(255,205,148);
    leds[41] = CRGB(255,205,148);
    leds[44] = CRGB(255,205,148);
    leds[32] = CRGB(32,156,101); //skin color

    for (int dot = 48; dot < 53; dot++) {
      leds[dot] = CRGB::Red;
    }
    
    FastLED.show();
    fill_solid( leds, NUM_LEDS, CRGB::Black);
    delay(d);


    //Frame 6
    leds[2] = CRGB::Purple;
    leds[5] = CRGB::Brown;
    leds[9] = CRGB::White;
    leds[10] = CRGB::Red;
    leds[11] = CRGB::Red;
    leds[12] = CRGB::Blue;
    leds[13] = CRGB::Blue;
    leds[17] = CRGB::DarkBlue;
    leds[18] = CRGB::Yellow;
    leds[19] = CRGB::Red;
    leds[20] = CRGB::Red;
    leds[21] = CRGB::Blue;
    leds[25] = CRGB(255,205,148); 
    leds[26] = CRGB(255,205,148);
    leds[27] = CRGB::Brown;
    leds[28] = CRGB::Brown;
    leds[32] = CRGB::Brown;
    leds[33] = CRGB::Brown;
    leds[34] = CRGB(255,205,148); 
    leds[35] = CRGB(255,205,148); 
    leds[36] = CRGB::Brown; 
    leds[37] = CRGB::Brown; 
    leds[42] = CRGB::Brown; 
    leds[43] = CRGB(255,205,148); 
    leds[44] = CRGB::Brown; 
    leds[45] = CRGB::Brown;
    for (int dot = 48; dot <53; dot++){
      leds[dot] = CRGB::Red;
    }
    for (int tod = 57; tod <61; tod++){
      leds[tod] = CRGB::Red;
    }
    

    FastLED.show();
    fill_solid(leds, NUM_LEDS, CRGB::Black);
    delay(d);

    
    //Frame 7
    leds[1] = CRGB::Purple;
    leds[14] = CRGB::Brown;
    leds[16] = CRGB::White;
    leds[9] = CRGB::Red;
    leds[10] = CRGB::Red;
    leds[11] = CRGB::Blue;
    leds[12] = CRGB::Blue;
    leds[13] = CRGB::Blue;
    leds[17] = CRGB::DarkBlue;
    leds[18] = CRGB::Yellow;
    leds[19] = CRGB::Red;
    leds[20] = CRGB::Red;
    leds[21] = CRGB::Blue;
    leds[25] = CRGB(255,205,148); 
    leds[26] = CRGB(255,205,148);
    leds[27] = CRGB::Brown;
    leds[28] = CRGB::Brown;
    leds[32] = CRGB::Brown;
    leds[33] = CRGB::Brown;
    leds[34] = CRGB(255,205,148); 
    leds[35] = CRGB(255,205,148); 
    leds[36] = CRGB::Brown; 
    leds[37] = CRGB::Brown; 
    leds[42] = CRGB::Brown; 
    leds[43] = CRGB(255,205,148); 
    leds[44] = CRGB::Brown; 
    leds[45] = CRGB::Brown;
    for (int dot = 48; dot <53; dot++){
      leds[dot] = CRGB::Red;
    }
    for (int tod = 57; tod <61; tod++){
      leds[tod] = CRGB::Red;
    }
    

    FastLED.show();
    fill_solid(leds, NUM_LEDS, CRGB::Black);

 
 



}
